load "days/2023_Day1.txt";
print DAY;

// Single line comments ignore the rest of the line

calibrationValues = 0;
print calibrationValues;
loop DAY lines: // Loop through all lines of DAY and set current line string value to variable: 'LINE'
	a = LINE; // Assign string variable to 'LINE'
	print a; // Print string variable a
	valueStr = "";
	loop a chars:  // Loop through all characters of LINE and set current character to variable: 'CHAR'
		print CHAR; // Print CHAR variable 
		if CHAR is DIGIT: // Logic check to compare if CHAR is '0' - '9'@34$
			valueStr = valueStr + CHAR; // Concatenate string
		else:end;
	loopend;
	calibrationValues = calibrationValues + valueStr as INTEGER; // Cast string to integer and add it to calibrationValues
loopend;